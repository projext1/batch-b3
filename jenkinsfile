
pipeline {
    agent any

    stages {
          stage('Clone') {
            steps {
             'git branch: 'main', credentialsId: '1368bde6-81c4-4c04-a24b-b74f0cc7b344', url: 'https://github.com/projext1/batch-b3.git''
            }
        }
        stage('Compile') {
            steps {
                bat 'javac Helloworld.java'
            }
        }

        stage('Run') {
            steps {
                bat 'java Helloworld'
            }
        }
    }
}
